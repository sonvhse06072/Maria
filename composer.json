{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^2.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/backup_migrate": "^5.0",
        "drupal/colorbox": "^2.1",
        "drupal/commerce": "~2.0",
        "drupal/commerce_variation_cart_form": "~1.0",
        "drupal/config_update": "^2.0@alpha",
        "drupal/core-composer-scaffold": "10.3.6",
        "drupal/core-project-message": "10.3.6",
        "drupal/core-recommended": "10.3.6",
        "drupal/ctools": "~3.0",
        "drupal/devel": "^5.0",
        "drupal/devel_entity_updates": "^4.1",
        "drupal/dropzonejs": "~2.0",
        "drupal/embed": "~1.0",
        "drupal/entity_browser": "~2.0",
        "drupal/entity_embed": "~1.0",
        "drupal/features": "~3.0",
        "drupal/feeds": "^3.0@RC",
        "drupal/field_group": "~3.0",
        "drupal/file_browser": "~1.0",
        "drupal/file_entity": "^2.0@RC",
        "drupal/flag": "~4.0",
        "drupal/imce": "^3.0",
        "drupal/jquery_ui_accordion": "^2.0",
        "drupal/jquery_ui_datepicker": "^1.0",
        "drupal/jquery_ui_draggable": "^2.0",
        "drupal/jquery_ui_droppable": "^2.0",
        "drupal/jquery_ui_tabs": "^2.0",
        "drupal/jquery_ui_tooltip": "^2.0",
        "drupal/ludwig": "^2.0",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/paragraphs": "^1.18",
        "drupal/pathauto": "^1.8",
        "drupal/recurly": "4.x-dev@dev",
        "drupal/seven": "^1.0",
        "drupal/shortcode": "^2.0",
        "drupal/tb_megamenu": "^3.0@alpha",
        "drupal/token": "~1.0",
        "drupal/upgrade_status": "^4.0",
        "drupal/video_embed_field": "~2.0",
        "drupal/views_flipped_table": "^2.0",
        "drupal/votingapi": "~3.0",
        "drupal/webform": "^6.2@beta",
        "drupal/youtube": "^2.0",
        "drush/drush": "^12.5"
    },
    "require-dev": {
        "drupal/core-dev": "^9 | ^10"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true,
            "tbachert/spi": true
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "htdocs/core": ["type:drupal-core"],
            "htdocs/libraries/{$name}": ["type:drupal-library"],
            "htdocs/modules/contrib/{$name}": ["type:drupal-module"],
            "htdocs/profiles/contrib/{$name}": ["type:drupal-profile"],
            "htdocs/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "htdocs/modules/custom/{$name}": ["type:drupal-custom-module"],
            "htdocs/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "htdocs/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "htdocs/"
            }
        },
        "drupal-core-project-message": {
            "include-keys": ["homepage", "support"],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",

                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
